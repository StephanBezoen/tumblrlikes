<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/photo_background"
    >

    <nl.acidcats.tumblrlikes.ui.widgets.InteractiveImageView
        android:id="@+id/photo"
        android:contentDescription="@string/pretty"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />

    <nl.acidcats.tumblrlikes.ui.widgets.PhotoActionDialog
        android:id="@+id/photo_action_dialog"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</FrameLayout>